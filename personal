task.wait(15)

_G.autoTeleport = false
_G.teleportToBestArea = true
_G.potatoGraphics = true
_G.customEggHatch = false

local __THINGS = workspace:WaitForChild("__THINGS")
local ReplicatedStorage = game:GetService("ReplicatedStorage")

--[[
if _G.autoTeleport then
    local currentStage = ""

    if not isfolder("AutoJoinPS") then
        makefolder("AutoJoinPS")
        task.wait(0.1)
        writefile("AutoJoinPS/CurrentStage.txt", "")
        task.wait(0.1)
    else
        local status, err = pcall(function ()
            currentStage = readfile("AutoJoinPS/CurrentStage.txt")
        end)
        if not status then
            writefile("AutoJoinPS/CurrentStage.txt", "")
            task.wait(0.1)
        end
    end
    
    if game.PlaceId == 8737899170 then
        if currentStage == "stage1done" then
            writefile("AutoJoinPS/CurrentStage.txt", "stage2done")
            task.wait(0.1)
            game:Shutdown()
        else
            writefile("AutoJoinPS/CurrentStage.txt", "stage3done")
            task.wait(0.1)
            ReplicatedStorage.Network.World2Teleport:InvokeServer()
        end
    elseif game.PlaceId == 16498369169 then
        local Players = game:GetService("Players")
        local PlayersCount = #Players:GetPlayers()
        if PlayersCount > 1 then
            writefile("AutoJoinPS/CurrentStage.txt", "stage1done")
            task.wait(0.1)
            ReplicatedStorage.Network.World1Teleport:InvokeServer()
        else
            writefile("AutoJoinPS/CurrentStage.txt", "joinedPS")
            task.wait(0.1)
        end
    else
        game:Shutdown()
    end
end
]]

if _G.teleportToBestArea then
    task.wait(5)
    if game.PlaceId == 17503543197 then
        ReplicatedStorage.Network.Teleports_RequestTeleport:InvokeServer("Prison HQ")
    end
end

if _G.potatoGraphics then
    task.wait(60)
    local lighting = game.Lighting
    local terrain = game.Workspace.Terrain
    terrain.WaterWaveSize = 0
    terrain.WaterWaveSpeed = 0
    terrain.WaterReflectance = 0
    terrain.WaterTransparency = 0
    lighting.GlobalShadows = false
    lighting.FogStart = 0
    lighting.FogEnd = 0
    lighting.Brightness = 0
    settings().Rendering.QualityLevel = "Level01"
    
    for i, v in pairs(game:GetDescendants()) do
        if v:IsA("Part") or v:IsA("Union") or v:IsA("CornerWedgePart") or v:IsA("TrussPart") then
            v.Material = "Plastic"
            v.Reflectance = 0
        elseif v:IsA("ParticleEmitter") or v:IsA("Trail") then
            v.Lifetime = NumberRange.new(0)
        elseif v:IsA("Explosion") then
            v.BlastPressure = 1
            v.BlastRadius = 1
        elseif v:IsA("Fire") or v:IsA("SpotLight") or v:IsA("Smoke") or v:IsA("Sparkles") then
            v.Enabled = false
        elseif v:IsA("MeshPart") then
            v.Material = "Plastic"
            v.Reflectance = 0
        end
    end
    
    for i, e in pairs(lighting:GetChildren()) do
        if e:IsA("BlurEffect") or e:IsA("SunRaysEffect") or e:IsA("ColorCorrectionEffect") or e:IsA("BloomEffect") or e:IsA("DepthOfFieldEffect") then
            e.Enabled = false
        end
    end
end

if _G.customEggHatch then
    task.wait(15)
    local args = {}
    
    local function getEggId()
        for i, v in pairs(__THINGS.CustomEggs:GetChildren()) do
            pcall(function()
                if not string.find(v.Name, "Highlight") and not string.find(v.Name, "Egg") then
                    args = {
                        [1] = v.Name,
                        [2] = 103
                    }
                end
            end)
        end
        return args[1]
    end
    
    spawn(function()
        repeat task.wait() until getEggId()
        while true do                
            pcall(function()
                ReplicatedStorage.Network.CustomEggs_Hatch:InvokeServer(unpack(args))
            end)
            wait(0.5)
        end
    end)
end
